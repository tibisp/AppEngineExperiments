<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

  <threadsafe>true</threadsafe>
  <sessions-enabled>false</sessions-enabled>
  <runtime>java8</runtime>

  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
  </system-properties>

  <static-files>
        <include path="/**.js" expiration="5d 0h"/>
        <include path="/**.png" expiration="5d 0h"/>
        <include path="/**.gif" expiration="5d 0h" />
        <include path="/**.jpg" expiration="5d 0h"/>
        <include path="/**.css" expiration="5d 0h"/>
        <include path="/js-lib/**.*" expiration="5d 0h"/>
        <include path="/js-scripts/**.*" />
        <include path="/style/**.*" expiration="5d 0h"/>
  </static-files>


</appengine-web-app>